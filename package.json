{
  "name": "webmathmech-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all build -p start watch:server",
    "start": "node build/server/index.js",
    "start:production": "NODE_ENV=production node build/server/index.js",
    "build": "npm-run-all build:clean -p build:server build:public",
    "build:clean": "rm -R build || :",
    "build:public": "copyfiles -e './**/index.html' -u 1 ./public/**/* build/static/",
    "build:server": "tsc --project server",
    "build:client": "webpack --config build/webpack.config.js",
    "build:production": "NODE_ENV=production run-s build build:client",
    "watch:server": "tsc --project server --watch"
  },
  "dependencies": {
    "config": "^3.2.4",
    "koa": "^2.11.0",
    "koa-compose": "^4.1.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.2.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/chokidar": "^2.1.3",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "24.0.23",
    "@types/koa": "^2.11.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa-webpack": "^5.0.1",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/webpack": "^4.41.0",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.0",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.2.1",
    "eslint": "^6.7.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
