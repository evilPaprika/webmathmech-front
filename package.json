{
    "name": "webmathmech-front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm-run-all build -p start watch:server",
        "start": "node build/server/index.js",
        "build": "npm-run-all build:clean -p build:server build:public",
        "build:clean": "rm -R build || :",
        "build:public": "copyfiles -e './**/index.html' -u 1 ./public/**/* build/static/",
        "build:server": "tsc --project server",
        "build:client": "webpack --config build/webpack.config.js",
        "build:production": "run-s build build:client",
        "watch:server": "tsc --project server --watch",
        "lint": "eslint --ext=.ts,.tsx ./",
        "lint:fix": "eslint --fix --ext=.ts,.tsx ./",
        "precommit": "npm run lint"
    },
    "dependencies": {
        "@types/html-webpack-plugin": "^3.2.1",
        "@types/koa": "^2.11.0",
        "@types/koa-compose": "^3.2.5",
        "@types/koa-logger": "^3.1.1",
        "@types/koa-router": "^7.0.42",
        "@types/koa-static": "^4.0.1",
        "@types/koa-webpack": "^5.0.1",
        "@types/mini-css-extract-plugin": "^0.8.0",
        "@types/node": "^12.12.14",
        "@types/react": "16.9.13",
        "@types/react-dom": "16.9.4",
        "@types/webpack": "^4.41.0",
        "babel-loader": "^8.0.6",
        "config": "^3.2.4",
        "copyfiles": "^2.1.1",
        "css-loader": "^3.2.1",
        "html-webpack-plugin": "^3.2.0",
        "koa": "^2.11.0",
        "koa-compose": "^4.1.0",
        "koa-logger": "^3.2.1",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "koa-webpack": "^5.2.4",
        "mini-css-extract-plugin": "^0.8.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-scripts": "^3.3.0",
        "ts-node": "^8.5.4",
        "typescript": "3.7.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/node": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@types/chokidar": "^2.1.3",
        "@types/jest": "24.0.23",
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
        "babel-eslint": "^10.0.3",
        "chokidar": "^3.3.0",
        "eslint": "^6.7.2",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-airbnb-typescript": "^6.3.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-json": "^2.0.1",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
