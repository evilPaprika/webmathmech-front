  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all build -p watch:server start",
    "start": "nodemon --watch build/server build/server/index.js",
    "start:production": "node build/server/index.js",
    "build": "npm-run-all build:clean -p build:server build:static",
    "build:clean": "rm -R build || :",
    "build:static": "copyfiles -e './**/index.html' -u 1 ./static/**/* build/static/",
    "build:server": "tsc --project server",
    "build:client": "webpack --config build/webpack.config.js",
    "build:production": "run-s build build:client",
    "watch:server": "tsc --project server --watch",
    "lint": "eslint --ext=.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext=.ts,.tsx ./"
  },
